<!--components/w-swiper/w-swiper.wxml-->
<swiper class="swiper ~d-swiper" circular autoplay
        interval="2500" duration="300"
        indicator-dots indicator-active-color="#ff5777">
  <block wx:for="{{list}}" wx:key="index">
    <swiper-item class="swiper-item">
      <image src="{{item.image || item}}" class="img"
             mode="widthFix"></image>
    </swiper-item>
  </block>
</swiper>
